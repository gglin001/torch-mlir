{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "terminal": "console",
            "program": "${workspaceFolder}/build/bin/dummy-opt",
            "args": [
                "--dummy-pass",
                // out
                "-o",
                "tools/dummy-opt/dummy.opt.mlir",
                // in
                "tools/dummy-opt/dummy.mlir",
            ],
            "initCommands": [
                "settings set target.disable-aslr false",
                // "command source ${workspaceRoot}/.lldbinit",
                // "settings set target.process.thread.step-avoid-regexp (.)std(.)*",
            ],
            "env": {
                // "GLOG_v": "10",
            },
        },
    ]
}